         <code code="REF" codeSystem="2.16.840.1.113883.5.4">
            <displayName value="Reference"/>
         </code>
         <value mediaType="text/plain"
                value="American Cancer Society. 2017. &quot;Cancer Prevention &amp; Early Detection Facts &amp; Figures 2017-2018.&quot; Atlanta: American Cancer Society."
                xsi:type="ED"/>
      </measureAttribute>
   </subjectOf>
   <subjectOf>
      <measureAttribute>
